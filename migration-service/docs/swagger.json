{
    "swagger": "2.0",
    "info": {
        "description": "This is a migration service for Stori Challenge",
        "title": "Migration Service | API Docs",
        "contact": {
            "name": "API Support",
            "email": "moralesaksel@gmail.com"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/migrate": {
            "post": {
                "description": "Migrate csv file to database",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Migrate"
                ],
                "summary": "Migrate csv file to database",
                "parameters": [
                    {
                        "type": "file",
                        "description": "CSV file to upload",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "csv file stored in database",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad request error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    },
                    "409": {
                        "description": "Conflict error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "domain.CustomError": {
            "description": "An error that includes a specific code and a message with more details.",
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "example": "error_code"
                },
                "message": {
                    "type": "string",
                    "example": "error message"
                }
            }
        }
    }
}