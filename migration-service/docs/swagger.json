{
    "swagger": "2.0",
    "info": {
        "description": "This is a migration service for Stori Challenge",
        "title": "Migration Service | API Docs",
        "contact": {
            "name": "API Support",
            "email": "moralesaksel@gmail.com"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/health": {
            "get": {
                "description": "Check if the service is up",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Health"
                ],
                "summary": "Health Check",
                "responses": {
                    "200": {
                        "description": "The service is up",
                        "schema": {
                            "$ref": "#/definitions/health.health"
                        }
                    }
                }
            }
        },
        "/migrate": {
            "post": {
                "description": "Migrate csv file to database",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Migrate"
                ],
                "summary": "Migrate csv file to database",
                "parameters": [
                    {
                        "type": "file",
                        "description": "CSV file to upload",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "processing csv file in the background, you will receive an email when the process is completed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad request error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    },
                    "409": {
                        "description": "Conflict error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/domain.CustomError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "domain.CustomError": {
            "description": "An error that includes a specific code and a message with more details.",
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "example": "error_code"
                },
                "message": {
                    "type": "string",
                    "example": "error message"
                }
            }
        },
        "health.health": {
            "description": "Detalles del estado de salud de la aplicaci√≥n.",
            "type": "object",
            "properties": {
                "status": {
                    "type": "string",
                    "example": "ok"
                }
            }
        }
    }
}